{
  "name": "react-photo-editor",
  "description": "React component for image editing with filters, rotate, zoom and more.",
  "private": false,
  "version": "2.1.2",
  "author": "Usama Chouhan (musama619)",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/musama619/react-photo-editor.git"
  },
  "homepage": "https://github.com/musama619/react-photo-editor#readme",
  "keywords": [
    "photo editor",
    "image editor",
    "react photo editor",
    "react image editor",
    "image editing",
    "react-photo-editor",
    "react-image-editor",
    "react component",
    "react pic editor",
    "react",
    "reactjs"
  ],
  "module": "./dist/react-photo-editor.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-photo-editor.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:css": "tailwindcss -o dist/index.css --minify",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.34.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.28",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.4.0",
    "vitest": "^0.34.3"
  }
}
